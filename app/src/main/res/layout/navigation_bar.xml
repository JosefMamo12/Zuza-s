<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="100dp"
    android:layout_alignParentBottom="true"
    android:background="#F2F3F5"
    android:gravity="center"
    tools:ignore="MissingConstraints,ContentDescription,UsingOnClickInXml"
    tools:showIn="@layout/activity_homepage">


    <ImageView
        android:id="@+id/logInImg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:onClick="onClick"
        android:padding="20dp"
        android:src="@drawable/ic_baseline_login_36" />


    <ImageView
        android:id="@+id/menuImg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:onClick="onClick"
        android:padding="20dp"
        android:src="@drawable/ic_baseline_menu_24" />

    <ImageView
        android:id="@+id/homeImg"
        android:onClick="onClick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:padding="20dp"
        android:src="@drawable/ic_baseline_home_24" />

    <ImageView
        android:id="@+id/menuFoodImg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:onClick="onClick"
        android:padding="20dp"
        android:src="@drawable/ic_baseline_restaurant_menu_24"/>

    <ImageView
        android:id="@+id/settingImg"
        android:onClick="onClick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:padding="20dp"
        android:src="@drawable/ic_baseline_settings_36"/>
</LinearLayout>